{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "start": "node index.js",
    "start:watch": "nodemon",
    "prestart:prod": "tsc",
    "start:prod": "node dist/main.js",
    "test": "jest"
  },
  "dependencies": {
    "@nestjs/common": "^4.4.0",
    "@nestjs/core": "^4.4.0",
    "@nestjs/microservices": "^4.4.0",
    "@nestjs/testing": "^4.4.0",
    "@nestjs/typeorm": "^2.0.0",
    "@nestjs/websockets": "^4.4.0",
    "mysql": "^2.15.0",
    "redis": "^2.7.1",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.4.0",
    "typeorm": "^0.1.11",
    "typescript": "^2.5.2"
  },
  "devDependencies": {
    "@types/jest": "^22.0.1",
    "@types/node": "^8.0.28",
    "jest": "^22.0.6",
    "nodemon": "^1.12.1",
    "supertest": "^3.0.0",
    "ts-jest": "^22.0.1",
    "ts-node": "^3.3.0"
  },
  "jest": {
    "verbose": true,
    "rootDir": "src",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(e2e-)?(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
